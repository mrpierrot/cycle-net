{
  "name": "cycle-ws",
  "version": "1.1.3",
  "description": "Websocket drivers for Cycle.js",
  "main": "dist/index.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha --require babel-register",
    "test:ws": "./node_modules/mocha/bin/mocha --require babel-register ./test/ws.spec.js",
    "test:io": "./node_modules/mocha/bin/mocha --require babel-register ./test/io.spec.js",
    "build": "babel src -d dist",
    "watch": "babel src -d dist -w",
    "pkg": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrpierrot/cycle-ws.git"
  },
  "keywords": [
    "node",
    "cycle.js",
    "ws",
    "websocket"
  ],
  "dependencies": {
    "@cycle/run": "^3.1.0",
    "xstream": "^10.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.5.1",
    "babel-register": "^6.24.1",
    "body-parser": "^1.17.2",
    "lodash": "^4.17.4",
    "mocha": "^3.4.2",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "ws": "^3.0.0"
  },
  "author": "Pierre Chabiland",
  "license": "MIT"
}
